@page "/userprofile"
@using ByteCuisine.Shared
@inject HttpClient Http


<div id="user_profile">
    <MyNavigation />
    <UserInfo />
    <ul>
        <li>tutaj lista</li>
        @foreach (var user in users)
        {
            <li>
                @user.User_id - @user.Username
                @if (user.PictureData == null)
                {
                    <img src="data:image/png;base64," alt="GeekforGeeks">
                }
                else
                {
                    <img src="data:image/png;base64,@Convert.ToBase64String(user.PictureData)" alt="user_image" />
                }
            </li>
        }
    </ul>
</div>

@code {
    List<Account> users = new List<Account>();

    protected override async Task OnInitializedAsync()
    {
        users = await Http.GetFromJsonAsync<List<Account>>("api/account");
    }
}
